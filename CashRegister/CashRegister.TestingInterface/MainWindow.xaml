<Window x:Class="CashRegister.TestingInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CashRegister.TestingInterface"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Height="450">
            <StackPanel Orientation="Horizontal" Height="50">
                <TextBox Name="txtFilename" FontSize="16" IsReadOnly="True" Width="700"></TextBox>
                <StackPanel Orientation="Vertical" Height="50" Width="90">
                    <Button Name="btnUpload" Click="btnUpload_Click" Content="Upload File" Height="25"></Button>
                    <Button Name="btnSubmit" Click="btnSubmit_Click" Content="Submit File" Height="25" IsEnabled="False"></Button>
                </StackPanel>
            </StackPanel>
            <TextBlock Name="txtOutput" Height="400" FontSize="20"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
